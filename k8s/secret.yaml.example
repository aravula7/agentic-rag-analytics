apiVersion: v1
kind: Secret
metadata:
  name: rag-analytics-secrets
  namespace: default
type: Opaque
stringData:
  # Database Credentials (Supabase)
  DB_HOST: "your-project.supabase.co"
  DB_PORT: "5432"
  DB_NAME: "postgres"
  DB_USER: "postgres"
  DB_PASSWORD: "your-database-password"
  
  # Supabase Storage (S3-Compatible)
  S3_ENDPOINT_URL: "https://your-project.supabase.co/storage/v1/s3"
  S3_BUCKET_NAME: "rag-reports"
  AWS_ACCESS_KEY_ID: "your-supabase-access-key-id"
  AWS_SECRET_ACCESS_KEY: "your-supabase-secret-access-key"
  
  # Upstash Redis
  UPSTASH_REDIS_REST_URL: "https://your-redis-url.upstash.io"
  UPSTASH_REDIS_REST_TOKEN: "your-upstash-token"
  
  # API Keys
  OPENAI_API_KEY: "sk-..."
  ANTHROPIC_API_KEY: "sk-ant-..."
  
  # Langfuse
  LANGFUSE_PUBLIC_KEY: "pk-lf-..."
  LANGFUSE_SECRET_KEY: "sk-lf-..."
  
  # Email
  SMTP_USER: "your-email@gmail.com"
  SMTP_PASSWORD: "your-app-specific-password"